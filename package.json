{
  "name": "documentation",
  "version": "1.0.0",
  "description": "a documentation generator",
  "main": "index.js",
  "scripts": {
    "test": "eslint index.js test/*.js test/streams/*.js && prova test/*.js test/streams/*.js",
    "cover": "istanbul cover prova test/*.js test/streams/*.js --dir $CIRCLE_ARTIFACTS"
  },
  "bin": {
    "documentation": "./bin/documentation.js"
  },
  "keywords": [
    "documentation",
    "jsdoc",
    "jsdoc3",
    "parser",
    "website",
    "formatter"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "dependencies": {
    "JSONStream": "^0.10.0",
    "ast-types": "^0.7.2",
    "doctrine": "^0.6.4",
    "espree": "^1.12.2",
    "extend": "^2.0.0",
    "handlebars": "^3.0.0",
    "highlight.js": "^8.4.0",
    "module-deps": "^3.7.3",
    "remarkable": "^1.6.0",
    "slugg": "^0.1.2",
    "stream-combiner": "^0.2.1",
    "through": "^2.3.6",
    "traverse": "^0.6.6",
    "vinyl": "^0.4.6",
    "vinyl-fs": "^1.0.0",
    "yargs": "^3.5.4"
  },
  "devDependencies": {
    "chdir": "0.0.0",
    "concat-stream": "^1.4.7",
    "eslint": "^0.17.1",
    "glob": "^5.0.2",
    "istanbul": "^0.3.7",
    "prova": "^2.1.1"
  }
}
